{
  "name": "@grafana-cloud/ui",
  "version": "2.0.1",
  "author": "Grafana Labs",
  "description": "Grafana Cloud shared UI",
  "repository": "https://github.com/grafana/grafana-cloud.git",
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.js.map",
    "dist/**/*.d.ts"
  ],
  "dependencies": {
    "@reduxjs/toolkit": "~1.9.7",
    "cidr-regex": "^4.0.3",
    "lodash": "~4.17.21",
    "p-queue": "~7.4.1",
    "react-redux": "~8.1.3",
    "uuid": "~9.0.1"
  },
  "scripts": {
    "build": "yarn webpack --config scripts/webpack/webpack.prod.js",
    "dev": "yarn webpack --progress --color --watch --config scripts/webpack/webpack.dev.js",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,less,pcss,scss,json,md,yaml,yml}\"",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx .",
    "lint:ts": "eslint . --ext .js,.tsx,.ts --cache",
    "lint:sass": "yarn stylelint '{public/sass,packages}/**/*.scss' --cache",
    "lint:fix": "yarn lint:ts --fix",
    "precommit": "yarn run lint-staged",
    "prettier:check": "prettier --check --list-different=false --log-level=warn \"**/*.{ts,tsx,scss,md,mdx}\"",
    "prettier:checkDocs": "prettier --check --list-different=false --log-level=warn \"docs/**/*.md\" \"*.md\" \"packages/**/*.{ts,tsx,scss,md,mdx}\"",
    "prettier:write": "prettier --list-different \"**/*.{js,ts,tsx,scss,md,mdx}\" --write",
    "start": "yarn dev --watch",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --onlyChanged",
    "test:ci": "jest --passWithNoTests --maxWorkers 4",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@babel/core": "~7.23.3",
    "@emotion/css": "^11.10.5",
    "@grafana/eslint-config": "^6.0.0",
    "@grafana/toolkit": "^9.0.2",
    "@grafana/tsconfig": "~1.3.0-rc1",
    "@grafana/ui": "~10.2.0",
    "@rtk-query/codegen-openapi": "^1.0.0",
    "@swc/core": "^1.2.144",
    "@swc/helpers": "^0.4.12",
    "@swc/jest": "^0.2.23",
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.9",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.7.1",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "babel-preset-es2015": "^6.24.1",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.29.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-webpack-plugin": "^3.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "husky": "^8.0.3",
    "indexof": "^0.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.3.1",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "lerna": "^7.4.2",
    "lint-staged": "^15.0.2",
    "path": "~0.12.7",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-data-table-component": "^7.5.4",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.7",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "~5.3.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-livereload-plugin": "^3.0.2",
    "webpack-merge": "^5.8.0"
  },
  "packageManager": "yarn@3.7.0"
}
